public class Person {
    idPerson: identificator
    firstName: string
    lastName: string
    address: Address
    age: integer
    weight: float
    height: double
    bloodType: char
    isEmployee: boolean
    school: School
    byteVariable: byte
    shortVariable: short
    longVariable: long
}

public class Address {
    idAddress: id
    street: string
    number: integer
    city: string
    country: string
    persons: Person
}

public class School {
    idSchool: id
    street: string
    number: integer
}

public class Grade {
    idGrade: id
    street: string
    number: integer
}